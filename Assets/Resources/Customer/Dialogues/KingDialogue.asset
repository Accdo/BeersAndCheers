  - speakerName: "\uD50C\uB808\uC774\uC5B4"
    text: "\uC5B4\uC11C\uC624\uC2ED\uC2DC\uC624. \uD3D0\uD558!"
    recommendedFood: {fileID: 0}
  - speakerName: "\uC655"
    text: "\uC694\uC0C8 \uB0B4\uAC00 \uAE30\uC6B4\uC774 \uC5C6\uACE0, \uC0C1\uD0DC\uAC00
      \uC88B\uC9C0 \uBABB\uD574\uC11C \uADF8\uB7F0\uB370"
    recommendedFood: {fileID: 0}
  - speakerName: "\uC655"
    text: "\uC774 \uAC00\uAC8C\uC5D0 \uBD80\uB4DC\uB7EC\uC6B4 \uC218\uD504\uAC00
      \uC788\uB294\uAC00?"
    recommendedFood: {fileID: 0}
  - speakerName: "\uD50C\uB808\uC774\uC5B4"
    text: "\uC608, \uC5EC\uB7EC\uC885\uB958\uC758 \uC2A4\uD504\uAC00 \uC788\uC0AC\uC635\uB2C8\uB2E4."
    recommendedFood: {fileID: 0}
  - speakerName: "\uC655"
    text: "\uADF8\uB807\uB2E4\uBA74, \uB4E0\uB4E0\uD55C \uACE0\uAE30\uAC00 \uB4E4\uC5B4\uC788\uB294
      \uC218\uD504\uB85C \uBD80\uD0C1\uD558\uB124!"
    recommendedFood: {fileID: 0}
  - speakerName: "\uD50C\uB808\uC774\uC5B4"
    text: "\uC608! \uC54C\uACA0\uC2B5\uB2C8\uB2E4. "
    recommendedFood: {fileID: 0}
